// 测试38: 类继承
// 验证: extends和super的映射

class Animal {
  constructor(name) {
    this.name = name
  }
  
  speak() {
    return `${this.name} makes a sound`
  }
}

class Dog extends Animal {
  constructor(name, breed) {
    super(name)
    this.breed = breed
  }
  
  speak() {
    return `${this.name} barks`
  }
}

const dog = new Dog("Buddy", "Golden")

div {
  p { dog.name }
  p { dog.breed }
  p { dog.speak() }
}

