// 测试47: 复杂状态管理
// 验证: 复杂状态和计算的映射

const state = {
  user: { name: "Alice", age: 25 },
  items: [1, 2, 3],
  flags: { active: true, loading: false }
}

const userName = state.user.name
const userAge = state.user.age
const isActive = state.flags.active
const total = state.items.reduce((sum, n) => sum + n, 0)

div {
  p { userName }
  p { userAge }
  p { isActive }
  p { total }
}

