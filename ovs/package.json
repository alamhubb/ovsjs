{
  "name": "ovsjs",
  "version": "0.1.7",
  "type": "module",
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./src/*": "./src/*"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsdown",
    "prepublishOnly": "npm run build"
  },
  "homepage": "https://github.com/alamhubb/ovs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alamhubb/ovs.git"
  },
  "contributors": [
    {
      "name": "alamhubb",
      "url": "https://github.com/alamhubb"
    }
  ],
  "keywords": [
    "ovs",
    "UI",
    "framework",
    "UI framework",
    "tsx",
    "no jsx",
    "no html",
    "oopUI",
    "flutterUI",
    "swiftUI"
  ],
  "author": "alamhubb",
  "description": "A technology similar to JSX, but unlike JSX which still uses XML syntax, OVS uses OOP syntax similar to Flutter and SwiftUI to develop UI.",
  "dependencies": {
    "slime-ast": "workspace:*",
    "slime-generator": "workspace:*",
    "slime-parser": "workspace:*",
    "slime-token": "workspace:*",
    "subhuti": "workspace:*"
  },
  "devDependencies": {
    "tsdown": "0.17.0-beta.6"
  },
  "bugs": {
    "url": "https://github.com/alamhubb/ovs/issues"
  },
