// 测试44: 条件渲染组件
// 验证: 条件渲染中组件的映射

const Message = (props) => {
  return div { props.text }
}

const isLoggedIn = true
const hasError = false

div {
  if (isLoggedIn) {
    Message({ text: "Welcome!" })
  }
  
  if (hasError) {
    Message({ text: "Error occurred" })
  } else {
    Message({ text: "All good" })
  }
}

