function getMessage() {
  return "All optimizations working!"
}

const message = getMessage()

div {
  h1 { "Simple Views Test" }
  p { message }
  
  div {
    span { "Nested" }
    span { " view" }
  }
}

div {
  h2 { "Complex Views Test" }
  
  const items = ["apple", "banana", "cherry"]
  for (let item of items) {
    p { item }
  }
}

div {
  h2 { "Conditional Test" }
  
  const showExtra = true
  if (showExtra) {
    p { "Extra content shown!" }
  }
}

